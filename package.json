{
  "author": "jktravis",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --runInBand --ci --coverage --testResultsProcessor ./testConfig/jestTrxProcessor.js",
    "test:e2e": "cypress open",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "prettier": "prettier \"{Scripts,Content}**/*.{ts,tsx,js,jsx,md,scss,css}\"",
    "validate": "npm-run-all --parallel test ts:check",
    "lint": "eslint Scripts/**/*.ts{,x} Scripts/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "eslint": "eslint",
    "clean-build": "rimraf ./build",
    "prebuild": "npm-run-all clean-build",
    "build": "npm-run-all gulp webpack:prod",
    "gulppack": "npm-run-all gulp webpack:dev",
    "gulp": "gulp",
    "ts": "tsc",
    "ts:check": "npm run ts -- --noEmit",
    "ts:watch": "tsc --watch",
    "webpack:prod": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=2816 webpack  --config webpack.config.prod.js",
    "webpack": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=3096 webpack --config webpack.config.dev.js",
    "webpack:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=2816 webpack --config webpack.config.dev.js",
    "webpack:watch": "npm run webpack -- --watch",
    "watch:all": "npm-run-all *:watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/is-prop-valid": "^0.8.2",
    "@emotion/styled": "^10.0.14",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "emotion": "^10.0.14",
    "emotion-theming": "^10.0.14",
    "formik": "^1.5.8",
    "localforage": "^1.7.3",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-icons": "^3.7.0",
    "react-select": "^3.0.4",
    "react-spinners": "0.5.11",
    "react-switch": "^5.0.1",
    "reactstrap": "^8.0.1",
    "sweetalert2": "8.14.0",
    "uuid": "^3.3.2",
    "xstate": "^4.6.7",
    "yup": "^0.27.0"
  },
  "description": "Test project for act issue",
